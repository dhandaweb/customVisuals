{
    "dataRoles": [
        {
            "displayName": "Activity",
            "name": "activity",
            "kind": "Grouping"
           
        },
        {
            "displayName": "Group",
            "name": "group",
            "kind": "Grouping"
        },
        {
            "displayName": "Time From",
            "name": "timeFrom",
            "kind": "Grouping"

        },
        {
            "displayName": "Time To",
            "name": "timeTo",
            "kind": "Grouping"
        },
        {
            "displayName": "Milestone",
            "name": "milestone",
            "kind": "Grouping"
        },
        {
            "displayName": "Additional",
            "name": "additional",
            "kind": "Measure"
          
        }
    ],
    "dataViewMappings": [
        {
            "table": {
                "rows": {
                    "dataReductionAlgorithm": {
                        "top": {
                            "count": 30000
                        }
                    },
                    "select": [
                        {
                            "for": {
                                "in": "timeFrom"
                            }
                        },
                        {
                            "for": {
                                "in": "timeTo"
                            }
                        },
                        {
                            "for": {
                                "in": "milestone"
                            }
                        },
                        {
                            "for": {
                                "in": "group"
                            }
                        },
                        {
                            "for": {
                                "in": "activity"
                            }
                        }
                    ]
                }
            },
            "conditions": [
                {
                    "timeFrom": {
                        "min": 0,
                        "max": 1
                    },
                    "timeTo": {
                        "min": 0,
                        "max": 1
                    },
                    "activity": {
                        "min": 0,
                        "max": 1
                    },
                    "additional": {
                        "min": 0,
                        "max": 1
                    },
                    "group": {
                        "min": 0,
                        "max": 1
                    }
                }
            ]
        }
    ],
    "objects": {
        "axis": {
            "displayName": "Axis",
            "properties": {
                "showXaxisBrush": {
                    "displayName": "X Axis Brush",
                    "type": { "bool": true }
                },
                "showYaxisBrush": {
                    "displayName": "Y Axis Brush",
                    "type": { "bool": true }
                }
            }
        },
        "milestone": {
            "displayName": "Milestone",
            "properties": {
                "milestoneSymbol": {
                    "displayName": "Symbol",
                    "type": {
                        "enumeration": [
                            {
                                "value": "circle",
                                "displayName": "Circle"
                            },
                            {
                                "value": "square",
                                "displayName": "Square"
                            },
                            {
                                "value": "diamond",
                                "displayName": "Diamond"
                            },
                            {
                                "value": "cross",
                                "displayName": "Cross"
                            },
                            {
                                "value": "triangle-up",
                                "displayName": "Triangle"
                            }
                        ]
                    }
                },
                "milestoneColor": {
                    "displayName": "Y Axis Brush",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                }
            }
        }
    }
} 
